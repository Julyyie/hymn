<%= link_to refuse_path(song, answer), data: {turbo_method: :patch}, class: "text-decoration-none", id:"wrong" do %>
  <i class="fa-sharp fa-regular fa-circle-xmark"></i>
<% end %>
<%= link_to accept_path(song, answer), data: {turbo_method: :patch}, class: "text-decoration-none", id:"correct" do %>
  <i class="fa-regular fa-circle-check"></i>
<% end %>